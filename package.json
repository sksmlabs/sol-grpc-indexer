{
  "name": "sol-grpc-indexer",
  "version": "1.0.0",
  "description": "",
  "main": "dist/src/index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/src/index.js",
    "clean": "rimraf dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "local:kafka:clear": "ts-node scripts/clear-topics.ts",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:app:logs": "docker-compose logs -f app",
    "docker:rebuild": "docker-compose down && docker-compose build && docker-compose up -d",
    "docker:kafka:logs": "docker-compose logs -f kafka",
    "docker:kafka:clear": "docker compose exec app node dist/scripts/clear-topics.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^24.3.1",
    "nodemon": "^3.1.10",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.13.4",
    "@prisma/client": "^6.16.1",
    "@triton-one/yellowstone-grpc": "^4.0.2",
    "bs58": "^6.0.0",
    "dotenv": "^17.2.2",
    "kafkajs": "^2.2.4",
    "pino": "^9.9.5",
    "pino-pretty": "^13.1.1",
    "prisma": "^6.16.1"
  }
}
